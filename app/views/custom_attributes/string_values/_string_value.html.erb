<% editable ||= false %>

<br/>
*** String ***
<% if not editable %>
	<%= string_value.name %>: <%= string_value.value %>
<% else %>
<%= form.fields_for :custom_attributes do |custom_fields| %>
	<%= string_value.name %><br/>
	<%= custom_fields.text_field "#{string_value.name}".to_sym, :value=>"#{string_value.value}" %>
<% end %>
<% end %>